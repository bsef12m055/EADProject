@model List<HolyQuran_WebProject.Models.Learner>
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
        }
@{
    ViewBag.Title = "GetLearners";
}

<h2>Remove Learners</h2>
<table>
<tr>
    <td>Id</td>
    <td>Name</td>
    <td>Password</td>
    <td>Remove</td>
</tr>
@for (int i = 0 ; i < Model.Count ; i++)
{
    <tr>
   <td>@Model[i].Id</td>
    <td>@Model[i].Name</td>
    <td>@Model[i].Password</td>
     <td><a href="/Adminaccount/RemoveLearner/@Model[i].Id">Remove</a></td>
    
    </tr>
    
}
</table>